#include <vector>
using namespace std;

class Solution {
public:
    int triangularSum(const vector<int>& nums) const {
        if(nums.size() == 1) return nums[0];
        vector<int> newNums(nums.size() - 1);
        for(size_t i = 0; i < nums.size() - 1; i++) {
            newNums[i] = (nums[i] + nums[i + 1]) % 10;
        }
        return triangularSum(newNums);
    }
};